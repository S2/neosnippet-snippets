snippet searchform
	<form method="post" id="form">
		<table class="default table-bordered table-striped element-left">
			<thead>
				<tr>
					<th>検索項目</th>
					<th >検索値</th>
				</tr>
			<tbody>
				<tr>
					<td class="index">
						<label>文字列検索[% error_id | mark_raw %]</label>
					</td>
					<td> 
						<input type="text" id="id" name="id" value="[% id %]">
					</td>
				</tr>
				<tr>
					<td class="index">
						<label>時間検索</label>
					</td>
					<td>
						<input type="text"  class="time "id="start_create_time" name="start_create_time" value="[% start_create_time %]"> ~ 
						<input type="text"  class="time "id="end_create_time" name="end_create_time" value="[% end_create_time %]">
					</td>
				</tr>
				<tr>
					<td class="index">
						<label>出力件数</label>
					</td>
					<td> 
						<select id="count" name="count">
							<option value=10 [% IF count == 10 %]selected=true[% END %] >10</option>
							<option value=20 [% IF count == 20 %]selected=true[% END %] >20</option>
							<option value=50 [% IF count == 50 %]selected=true[% END %] >50</option>
						</select>件ずつ出力
					</td>
				</tr>
				<tr>
					<td class="index">
						<label>ソート</label>
					</td>
					<td>
						<select id="sort" name="sort">
							[% FOR id IN sort_select.keys().sort() %]
								<option value=[% id %] [% IF sort == id %]selected=true[% END %]>[% sort_select.${id}[0] %]</option>
							[% END %]
						</select>
					</td>
				</tr>
				<tr>
					<td class="index">
					</td>
					<td class="index">
						<button type="submit" class="btn">Search</button>
					</td>
				</tr>
			</tbody>
		</table>
	</form>
	
	<script>
		$("#start_create_time").datepicker({ dateFormat: 'yy-mm-dd' });
		$("#end_create_time").datepicker({ dateFormat: 'yy-mm-dd' });
	</script>

